for job_id in $(cat ../harmony-job-ids); do
    echo $job_id;
    
    echo "" > "urls-${job_id}";

    for page_link in $(cat "urls_pages-${job_id}"); do
        curl -Lnbj $page_link \
            | jq '.links[] | select( .type == "application/x-hdf5" ) | .href' \
            >> "urls-${job_id}"
    done

done
